{
  "version": 2,
  "name": "comicpal-api",
  "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
  
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ],
      "destination": "/index.js"
	  
    }    
  ]
}
