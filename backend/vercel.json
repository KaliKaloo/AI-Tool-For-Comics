{
    "version": 2,
    "name": "comicpal-api",
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" }
    ],
    
	"rewrite": [
		{ "source": "/:path((?!uk/).*)", "destination": "/index.js" }
	],
	"headers": [
        {
            "source": "/:path((?!uk/).*)",
            "headers" : [
                {
                    "key" : "Cache-Control",
                    "value" : "no-cache"
                }
            ]
        }
    ]
  }